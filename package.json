{
  "name": "@bpiwek/frontbox-kss-styleguide",
  "version": "0.0.6",
  "description": "Fronbox KSS Styleguide",
  "scripts": {
    "playground": "yarn style && yarn scripts && kss playground/ --config kss-config.json",
    "style": "node-sass src/style/index.scss dist/kss-assets/index.css",
    "scripts": "browserify src/scripts/index.ts -p [ tsify --noImplicitAny ] > dist/kss-assets/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "framework",
    "frontbox",
    "kss",
    "documentation",
    "styleguide"
  ],
  "author": "Bartosz Piwek",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.3.34",
    "browserify": "^16.5.1",
    "github-markdown-css": "^4.0.0",
    "kss": "^3.0.0",
    "node-sass": "4.13.1",
    "reset-css": "^5.0.1",
    "scrollspy-js": "^0.1.3",
    "ts-node": "^8.8.1",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node": "^13.9.5",
    "highlight.js": "^9.18.1",
    "prismjs": "^1.19.0",
    "sass.js": "^0.11.1"
  }
}