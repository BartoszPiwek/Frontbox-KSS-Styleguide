{
  "name": "@bpiwek/frontbox-kss-styleguide",
  "version": "0.0.9",
  "description": "Fronbox KSS Styleguide",
  "scripts": {
    "build:dev:css": "node-sass src/style/index.scss dist/kss-assets/index.css",
    "build:prod:css": "postcss --use autoprefixer < dist/kss-assets/index.css | postcss --use cssnano > dist/kss-assets/index.min.css",
    "build:dev:js": "browserify src/scripts/index.ts -p [ tsify --noImplicitAny ] > dist/kss-assets/index.js",
    "playground": "kss playground/ --config kss-config.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn build:dev:css && yarn build:prod:css",
    "publish": "yarn build && npm publish --access public"
  },
  "keywords": [
    "framework",
    "frontbox",
    "kss",
    "documentation",
    "styleguide"
  ],
  "main": "dist/index.js",
  "author": "Bartosz Piwek",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.3.34",
    "autoprefixer": "^9.7.5",
    "browserify": "^16.5.1",
    "cssnano": "^4.1.10",
    "github-markdown-css": "^4.0.0",
    "interactjs": "^1.9.7",
    "kss": "^3.0.0",
    "node-sass": "4.13.1",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "reset-css": "^5.0.1",
    "scrollspy-js": "^0.1.3",
    "ts-node": "^8.8.1",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node": "^13.9.5",
    "highlight.js": "^9.18.1",
    "prismjs": "^1.19.0",
    "sass.js": "^0.11.1",
    "strip-css-comments": "^4.1.0"
  }
}
